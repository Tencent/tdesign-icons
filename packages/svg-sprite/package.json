{
    "name": "wedocs-icons-svgSprite",
    "version": "0.0.0",
    "description": "Icons used like a svgSprite",
    "contributors": [
        "wendellhu",
        "weijiyang"
    ],
    "files": [
        "dist/",
        "esm/"
    ],
    "main": "dist/index.js",
    "module": "esm/index.js",
    "sideEffects": false,
    "scripts": {
        "build": "yarn build:esm && yarn build:cjs",
        "build:cjs": "ncc build src/index.ts -o dist -m -e react",
        "build:esm": "tsc --module ES6 --outDir esm"
    },
    "peerDependencies": {
        "react": "^16.0.0",
        "react-dom": "^16.0.0"
    },
    "devDependencies": {
        "@types/react": "^17.0.0",
        "@vercel/ncc": "^0.27.0",
        "camelcase": "^6.2.0",
        "cross-env": "^7.0.3",
        "typescript": "^4.1.3"
    }
}
