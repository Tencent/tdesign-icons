    "@types/svg-parser": "catalog:types",
    "@types/svgo": "catalog:types",
    "@types/through2": "catalog:types",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "camelcase": "catalog:",
    "cross-env": "catalog:",
    "decamelize": "catalog:",
    "del": "catalog:",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "catalog:",
